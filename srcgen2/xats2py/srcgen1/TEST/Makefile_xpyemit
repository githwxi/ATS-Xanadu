# Makefile for PY-emission
########################################################################
########################################################################
CAT=cat
ECHO=echo
PRINTF=printf
########################################################################
BUN=bun
NODE=node
PYTHON=python3
########################################################################
########################################################################
all:: test00
all:: test01
########################################################################
########################################################################
testall:: test00; $(PYTHON) $<_out.py
testall:: test01; $(PYTHON) $<_out.py
########################################################################
########################################################################
SRCGEN2_SRCGENX=\
$(XATSHOME)/srcgen2/xats2py/srcgenx
