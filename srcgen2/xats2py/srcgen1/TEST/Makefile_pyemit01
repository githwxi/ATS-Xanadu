$(CAT) $(XATS2PY_XSHARED)/xats2py_py1emit.py >> OUTS/$@_out.py
test00:: ; \
$(CAT) $(XATS2PY_XSHARED)/srcgen2_prelude.py >> OUTS/$@_out.py
############
test00:: \
test00_xats2py.dats; \
$(PYTHON) $(XATS2PY_PYEMIT01) ______void______ $< >> OUTS/$@_out.py
