# Makefile for XDPNDCY
########################################################################
########################################################################
CAT=cat
ECHO=echo
NODE=node
PRINTF=printf
########################################################################
########################################################################
all:: test00
all:: test01
########################################################################
########################################################################
XDPNDCY_OUTPUT00=./../UTIL/xdpndcy_output00_ats3.js
XDPNDCY_OUTPUT01=./../UTIL/xdpndcy_output01_ats3.js
########################################################################
########################################################################
test00:: \
test00_xdpndcy.dats; \
$(NODE) --stack-size=8800 $(XDPNDCY_OUTPUT00) $< >> $@_out.00
########################################################################
test01:: \
test01_xdpndcy.dats; \
$(NODE) --stack-size=8800 $(XDPNDCY_OUTPUT00) $< >> $@_out.00
########################################################################
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f test??_out.00
