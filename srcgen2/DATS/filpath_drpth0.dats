#include
"./../HATS/xatsopt_dats.hats"
(* ****** ****** *)
#define
ATS_PACKNAME
"ATS3.XANADU.xatsopt-20220500"
(* ****** ****** *)
#staload "./../SATS/filpath.sats"
(* ****** ****** *)

local

datatype
drpth =
DRPTH of
(strn(*name*))
#absimpl drpth_tbox = drpth

in//local

#implfun
drpth_get_name(dpx) =
let
val+DRPTH(nam) = dpx in nam
end // end of [drpth_get_name]

#impltmp
drpth_make_name(nam) = DRPTH(nam)

end (*local*) // end of [local]

