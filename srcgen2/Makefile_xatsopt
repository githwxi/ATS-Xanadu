AR=ar
CP=cp
MV=mv
CC=gcc
SED=sed
CPF=cp -f
MVF=mv -f
RMF=rm -f
WCL=wc -l

######
all::
######
all:: all_sats
######
all:: all_dats
######

######
#
# HX:
# srcgen1 is
# implemented in ATS2
#
XATSOPT=\
$(XATSHOME)/srcgen1/bin/xatsopt
#
# HX-2024-07-12: Kudos to Gavin!
#
# XATSOPT=\
# nix run \
# github:GZGavinZhao/ATS-Xanadu/nixify\#xatsopt \
# --extra-experimental-features flakes \
# --extra-experimental-features nix-command --
#
######

