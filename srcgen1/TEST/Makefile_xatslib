######

######
XATSOPT=\
./../bin/xatsopt
######
XATSLIB=./../../xatslib
######
XATSLIB1=./../../srcgen1/xatslib
######

######

all1:: all1_libcats
all1:: all1_githwxi
all1:: all1_haskell
all1:: all1_githwxi_CATS

############
all1_libcats:: ; \
$(XATSOPT) -s $(XATSLIB1)/libcats/SATS/libcats.sats
all1_libcats:: ; \
$(XATSOPT) -s $(XATSLIB1)/libcats/SATS/synoug0.sats
############
all1_libcats:: ; \
$(XATSOPT) -d $(XATSLIB1)/libcats/DATS/synoug0.dats
############
all1_githwxi:: ; \
