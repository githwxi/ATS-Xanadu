all1_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB1)/githwxi/DATS/g00iout.dats
all1_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB1)/githwxi/DATS/myrand0.dats
all1_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB1)/githwxi/DATS/mygist0.dats
all1_githwxi:: ; \
$(XATSOPT) -d $(XATSLIB1)/githwxi/DATS/mytest0.dats
############
all1_githwxi:: ; \
