########################################################################
########################################################################
# Makefile for
# ATS3-XANADU/
# srcgen1/prelude/DATS/CATS/JS/.
########################################################################
########################################################################
CAT=cat
ECHO=echo
PRINTF=printf
########################################################################
########################################################################
all:: precats
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f OUTS/*_dats.js
########################################################################
XATSJSD=\
../../../../xats2js/srcgen1
XATS2JS=${XATSJSD}/bin/xats2js
########################################################################
########################################################################
precats:: ; \
$(PRINTF) "// %s\n" "`date`" > OUTS/$@_dats.js
precats:: precats.dats; \
$(XATS2JS) --output-a OUTS/$@_dats.js --dynamic $<
precats:: ; \
$(PRINTF) "////////////////////////////////////" >> OUTS/$@_dats.js
precats:: ; \
$(PRINTF) "////////////////////////////////////.\n" >> OUTS/$@_dats.js
precats:: ; \
$(PRINTF) "////////////////////////////////////" >> OUTS/$@_dats.js
precats:: ; \
$(PRINTF) "////////////////////////////////////.\n" >> OUTS/$@_dats.js
########################################################################
########################################################################
##################### end of [Makefile_jsemit01] #######################
