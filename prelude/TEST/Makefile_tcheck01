OUTSATS := \
$(patsubst %.sats, \
  OUTS/%_sats_out.00, $(PRESATS))
OUTDATS := \
$(patsubst %.dats, \
  OUTS/%_dats_out.00, $(PREDATS))
OUTSATS_vt := \
$(patsubst %.sats, \
  OUTS/%_sats_out.00, $(PRESATS_vt))
########################################################################
########################################################################
all:: \
presats
presats: \
$(OUTSATS)
OUTS/%_sats_out.00: \
../SATS/%.sats; $(NODE) --stack-size=4096 $(SRCGEN2_XATSOPT) $<
########################################################################
all:: \
presats_vt
presats_vt: \
$(OUTSATS_vt)
OUTS/%_sats_out.00: \
../SATS/VT/%.sats; $(NODE) --stack-size=4096 $(SRCGEN2_XATSOPT) $<
########################################################################
