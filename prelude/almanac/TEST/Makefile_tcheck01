########################################################################
########################################################################
# Makefile for type-checking
########################################################################
########################################################################
NODE=node
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
########################################################################
########################################################################
MYROOT="./../../.."
########################################################################
########################################################################
all::
########################################################################
all:: \
pre2026_sats
pre2026_sats: \
./../SATS/pre2026.sats; \
$(NODE) --stack-size=4096 $(XATSOPT_TCHECK01) $<
########################################################################
all:: \
pre2026_dats
pre2026_dats: \
./../DATS/pre2026.dats; \
$(NODE) --stack-size=4096 $(XATSOPT_TCHECK01) $<
########################################################################
########################################################################
XATSOPT_TCHECK01=\
$(MYROOT)/xassets/JS/xatsopt/xatsopt_tcheck01_ats3_opt1.js
########################################################################
###################### end of [Makefile_tcheck01] ######################
