########################################################################
NODE=node
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
########################################################################
########################################################################
MYROOT="./../../.."
SRCGEN2_XATSOPT=$(MYROOT)/srcgen2/UTIL/xatsopt_tcheck01_ats2.js
########################################################################
########################################################################
all::
########################################################################
all:: \
pre2026_sats
pre2026_sats: \
./../SATS/pre2026.sats; \
$(NODE) --stack-size=4096 $(SRCGEN2_XATSOPT) $<
########################################################################
all:: \
pre2026_dats
pre2026_dats: \
./../DATS/pre2026.dats; \
$(NODE) --stack-size=4096 $(SRCGEN2_XATSOPT) $<
