#impltmp
{ a0:t0
, a1:t0 }
g_cmp
<t0up(a0,a1)>
  (x0, x1) =
let
//
val sgn =
g_cmp<a0>(x0.0, x1.0)
//
in//let
if
(sgn = 0) then
g_cmp<a1>(x0.1, x1.1) else sgn
end//let//end-of-[g_cmp<t0up(...)>]
