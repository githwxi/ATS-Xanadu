########################################################################
########################################################################
# Makefile for
# ATS3-XANADU/prelude/DATS/CATS/PY/.
########################################################################
########################################################################
CAT=cat
BUN=bun
ECHO=echo
NODEJS=node
PRINTF=printf
PYTHON=python3
########################################################################
########################################################################
all:: precats
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f OUTS/*_dats.py
########################################################################
########################################################################
XASSETS=./../../../../xassets
# SRCGEN2=./../../../../srcgen2
# XSHARED=$(SRCGEN2)/xats2py/srcgen1/xshared
########################################################################
#
XATS2PY_PYEMIT01=\
$(XASSETS)/JS/xats2py/xats2py_pyemit01_ats2_opt1.js
XATS2PY_PYEMIT01=\
$(XASSETS)/JS/xats2py/xats2py_pyemit01_ats3_opt1.js
#
########################################################################
########################################################################
precats:: ; \
$(PRINTF) "## %s\n" "`date`" > OUTS/$@_dats.py
precats:: precats.dats; \
$(NODEJS) --stack-size=8192 $(XATS2PY_PYEMIT01) $< >> OUTS/$@_dats.py
########################################################################
########################################################################
##################### end of [Makefile_pyemit01] #######################
