########################################################################
########################################################################
# Makefile for
# ATS3-XANADU/prelude/DATS/CATS/JS/.
########################################################################
########################################################################
CAT=cat
BUN=bun
ECHO=echo
NODE=node
PRINTF=printf
PYTHON=python3
########################################################################
########################################################################
all:: precats
########################################################################
clean:: ; rm -f *~
cleanall:: ; rm -f *~
cleanall:: ; rm -f OUTS/*_dats.js
########################################################################
########################################################################
XASSETS=./../../../../xassets
# SRCGEN2=./../../../../srcgen2
# XSHARED=$(SRCGEN2)/xats2js/srcgen1/xshared
########################################################################
#
XATS2JS_JSEMIT01=\
$(XASSETS)/JS/xats2js/xats2js_jsemit01_ats3_opt1.js
#
########################################################################
########################################################################
precats:: ; \
$(PRINTF) "// %s\n" "`date`" > OUTS/$@_dats.js
precats:: precats.dats; \
$(NODE) --stack-size=8192 $(XATS2JS_JSEMIT01) $< >> OUTS/$@_dats.js
########################################################################
########################################################################
##################### end of [Makefile_jsemit01] #######################
