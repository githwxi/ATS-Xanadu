srcgen2_xpyemit_xats2js_jsemit01: ; \
XATS2PY_PYEMIT00=./../../../../$(XATS2PY_PYEMIT00) \
time make -C srcgen2/xats2js/srcgen1/UTIL -f Makefile_xpyemit xats2js_jsemit01
#
##################################################################
