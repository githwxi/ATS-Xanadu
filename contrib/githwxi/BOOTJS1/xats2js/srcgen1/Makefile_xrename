intrep1_utils0_dats: ; $(SED) -i -e 's/jstnm/js1006tnm/g' $@_out.js
##################################################################
trxd3i0_dats: ; $(SED) -i -e 's/jstnm/js1007tnm/g' $@_out.js
trxd3i0_myenv0_dats: ; $(SED) -i -e 's/jstnm/js1008tnm/g' $@_out.js
