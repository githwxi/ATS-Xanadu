##################################################################
trxi0i1_dats: ; $(SED) -i -e 's/jstnm/js1011tnm/g' $@_out.js
trxi0i1_myenv0_dats: ; $(SED) -i -e 's/jstnm/js1012tnm/g' $@_out.js
trxi0i1_dynexp_dats: ; $(SED) -i -e 's/jstnm/js1013tnm/g' $@_out.js
trxi0i1_decl00_dats: ; $(SED) -i -e 's/jstnm/js1014tnm/g' $@_out.js
##################################################################
xats2js_dats: ; $(SED) -i -e 's/jstnm/js1015tnm/g' $@_out.js
xats2js_myenv0_dats: ; $(SED) -i -e 's/jstnm/js1016tnm/g' $@_out.js
xats2js_dynexp_dats: ; $(SED) -i -e 's/jstnm/js1017tnm/g' $@_out.js
xats2js_decl00_dats: ; $(SED) -i -e 's/jstnm/js1018tnm/g' $@_out.js
xats2js_tmplib_dats: ; $(SED) -i -e 's/jstnm/js1019tnm/g' $@_out.js
##################################################################
js1emit_dats: ; $(SED) -i -e 's/jstnm/js1020tnm/g' $@_out.js
js1emit_utils0_dats: ; $(SED) -i -e 's/jstnm/js1021tnm/g' $@_out.js
