########################################################################
SRCGEN2=./../../../../srcgen2
########################################################################
OBJSATS := \
$(patsubst %.sats, OUTS/%_sats_out.00, $(SRCSATS))
OBJDATS := \
$(patsubst %.dats, OUTS/%_dats_out.00, $(SRCDATS))
########################################################################
########################################################################
all:: all_sats
all:: all_dats
all_sats:: $(OBJSATS)
all_dats:: $(OBJDATS)
########################################################################
clean:: ; rm -f *~
########################################################################
cleanall:: ; rm -f *~
cleanall:: ; rm -f OUTS/*_sats_out.00
cleanall:: ; rm -f OUTS/*_dats_out.00
########################################################################
########################################################################
#
OUTS/%_sats_out.00: \
$(SRCGEN2)/SATS/%.sats; \
$(NODE) --stack-size=8192 $(XATSOPT_TCHECK00) $< > $@
#
OUTS/%_dats_out.00: \
$(SRCGEN2)/DATS/%.dats; \
$(NODE) --stack-size=8192 $(XATSOPT_TCHECK00) $< > $@
#
########################################################################
########################################################################
XATSOPT_TCHECK00=./../UTIL/xatsopt_tcheck00_dats_out.js
########################################################################
########################################################################
